{
    "version": "11.1.0-9.0.0-r3",
    "description": "It's a free C and C++ compiler for Microsoft Windows",
    "homepage": "http://winlibs.com",
    "architecture": {
        "64bit": {
            "url": "https://github.com/brechtsanders/winlibs_mingw/releases/download/11.1.0-12.0.0-9.0.0-r3/winlibs-x86_64-posix-seh-gcc-11.1.0-mingw-w64-9.0.0-r3.7z",
            "hash": "eedc0481e8cd4a4a82ffc91baec4412347d0bd4baaf546315be7a9b113316843"
        }
    },
    "extract_dir": "mingw64",
    "env_add_path": "bin",
    "checkver": {
        "regex": "gcc-(?<gcc>[\\d.]+)-llvm-(?<llvm>[\\d.]+)-mingw-w64-(?<mingw>[\\d.]+)-(?<release>r\\d+)",
        "replace": "${gcc}-${mingw}-${release}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/brechtsanders/winlibs_mingw/releases/download/$matchGcc-$matchLlvm-$matchMingw-$matchRelease/winlibs-x86_64-posix-seh-gcc-$matchGcc-mingw-w64-$matchMingw-$matchRelease.7z"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
